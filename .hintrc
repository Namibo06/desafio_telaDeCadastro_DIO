{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 104",
    "not firefox <= 105",
    "not ios_saf <= 14.8",
    "not opera <= 90",
    "not safari <= 14.1",
    "not opera <= 91"
  ],
  "hints": {
    "no-inline-styles": "off",
    "meta-charset-utf-8": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "input[autocapitalize]"
        ]
      }
    ]
  }
}